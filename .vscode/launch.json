{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "--unstable",
        "--import-map=import_map.json",
        "-A",
        "./postprocess.js"
      ],
      "attachSimplePort": 9229
    }
  ]
}
